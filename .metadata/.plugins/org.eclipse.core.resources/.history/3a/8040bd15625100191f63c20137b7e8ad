package v1;

import java.util.ArrayList;

public class Indice 
{
	public ArrayList<String> vals;
	
	public Indice(ArrayList<String> vals)
	{
		this.vals = vals;
	}
	
	public void show()
	{
		for(String s:vals) {System.out.print(String.format("%-40s", (s.length()>40?s.substring(0,40):s)));}
		System.out.println();
	}
}
