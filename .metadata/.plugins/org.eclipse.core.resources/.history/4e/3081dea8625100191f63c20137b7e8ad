package v1;

import java.util.ArrayList;

public class Indice 
{
	public String nom;
	public ArrayList<String> vals;
	
	public Indice(ArrayList<String> vals)
	{
		this.nom = this.vals.get(vals.size()<6?0:1);
		this.vals = vals;
	}
	
	public void show()
	{
		for(String s:vals) {System.out.print("x"+String.format("%-40s", (s.length()>39?s.substring(0,39):s)));}
		System.out.println();
	}
}
